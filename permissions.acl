/**
 * Access control list.
 */

rule SupplierRule {
    description: "Allow all participants access to all resources"
    participant: "ANY"
    operation: ALL
    resource: "sdc.network.*"
    action: ALLOW
}

rule SystemACL {
  description:  "System ACL to permit all access"
  participant: "ANY"
  operation: ALL
  resource: "org.hyperledger.composer.system.**"
  action: ALLOW
}
